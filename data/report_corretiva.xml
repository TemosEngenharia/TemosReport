<odoo>

    <record id="temos_corretiva_report_py3o" model="ir.actions.report.xml">
      <field name="name">Relatório Corretivas</field>
      <field name="type">ir.actions.report.xml</field>
      <field name="model">corretiva.entry_form</field>
      <field name="report_name">Corretivas</field>
      <field name="report_type">py3o</field>
      <field name="py3o_filetype">odt</field>
      <field name="py3o_is_local_fusion" eval="1"/>
      <field name="py3o_filetype">odt</field>
      <field name="module">temos_report</field>
      <field name="py3o_template_fallback">data/report_corretiva.odt</field>
    </record>

    <record id="temos_corretiva_report_py3o_print_action" model="ir.values">
      <field eval="'action'" name="key" />
      <field eval="'client_print_multi'" name="key2" />
      <field name="model">corretiva.entry_form</field>
      <field name="name">Temos Relatório Corretiva</field>
      <field eval="'ir.actions.report.xml,'+str(temos_corretiva_report_py3o)" name="value" />
    </record>

</odoo>

